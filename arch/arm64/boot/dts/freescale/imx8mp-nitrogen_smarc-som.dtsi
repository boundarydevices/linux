		interrupt-controller;
		#interrupt-cells = <0x2>;
		microchip,irq-mirror;
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_i2c6c_mcp23018>;
		reg = <0x20>;
		reset-gpios = GP_I2C6C_MCP23018_RESET;
	};
};

