			MX8MP_IOMUXC_NAND_CE2_B__GPIO3_IO03	0x100
		>;
	};

	pinctrl_usb3_1: usb3-1grp {
		fsl,pins = <
#define GP_USB3_1_HUB_RESET	<&gpio3 6 GPIO_ACTIVE_LOW>
			MX8MP_IOMUXC_NAND_DATA00__GPIO3_IO06	0x100
		>;
	};

	pinctrl_usdhc1: usdhc1grp {
