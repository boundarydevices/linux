		>;
	};

	pinctrl_i2c6c_mcp23018: i2c6c-mcp23018grp {
		fsl,pins = <
#define GPIRQ_I2C6C_MCP23018	<&gpio4 22 IRQ_TYPE_LEVEL_LOW>
			MX8MP_IOMUXC_SAI2_RXC__GPIO4_IO22		0x1c0
