		csi_id = <1>;
		mclk = <25000000>;
		mipi_csi;
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_i2c3_ov5640_mipi>;
		pwn-gpios = GP_CSI2_OV5640_MIPI_POWER_DOWN;
		reg = <0x3c>;
		rst-gpios = GP_CSI2_OV5640_MIPI_RESET;
		status = "okay";

		port {
			ov5640_mipi2_ep: endpoint {
				remote-endpoint = <&mipi2_ov5640_ep>;
			};
		};
	};
};

&i2c4 {
	clock-frequency = <100000>;
	pinctrl-names = "default", "gpio";
	pinctrl-0 = <&pinctrl_i2c4>;
	pinctrl-1 = <&pinctrl_i2c4_1>;
	scl-gpios = GP_I2C4_SCL;
	sda-gpios = GP_I2C4_SDA;
	status = "okay";

	mipi_to_lvds: mipi-to-lvds@2c {
		clocks = <&mipi_dsi_phy 0>;
		clock-names = "mipi_clk";
		compatible = "ti,sn65dsi83";
		display = <&lcdif>;
		display-dsi = <&fb_mipi>;
		enable-gpios = GP_I2C4_SN65DSI83_EN;
		interrupts-extended = GPIRQ_I2C4_SN65DSI83;
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_i2c4_sn65dsi83>;
		reg = <0x2c>;
		status = "disabled";
	};

	spi_lcd: spi@2f {
		compatible = "nxp,sc18is602b";
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_sc18is602b>;
		reg = <0x2f>;
		reset-gpios = GP_SC18IS602B_RESET;
		status = "disabled";
		#address-cells = <1>;
		#size-cells = <0>;

		spidev_lcd: spidev@0 {
			compatible = "spidev";
			spi-cpha;
			spi-cpol;
			spi-max-frequency = <20000000>;
			reg = <0>;
		};
	};

	ts_ft5x06: touchscreen@38 {
		compatible = "edt,ft5x06-ts";
		interrupts-extended = GPIRQ_I2C4_FT5X06;
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_i2c4_ft5x06>;
		reg = <0x38>;
		reset-gpios = GP_I2C4_FT5X06_RESET;
		wakeup-gpios = GP_I2C4_FT5X06_WAKE;
	};

	ts_ft7250: touchscreen_ft7250@38 {
		compatible = "edt,ft7250";
		interrupts-extended = GPIRQ_I2C4_FT7250;
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_i2c4_ft7250>;
		reg = <0x38>;
		reset-gpios = GP_I2C4_FT7250_RESET;
		wakeup-gpios = GP_I2C4_FT7250_WAKE;
		status = "disabled";
	};

	ts_goodix: touchscreen@5d {
		compatible = "goodix,gt9271";
		esd-recovery-timeout-ms = <2000>;
		interrupts-extended = GPIRQ_GT911;
		irq-gpios = GP_GT911_IRQ;
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_i2c4_gt911>;
		reg = <0x5d>;
		reset-gpios = GP_GT911_RESET;
	};

	ts_st1633i: touchscreen@55 {
		compatible = "sitronix,st1633i";
		interrupts-extended = GPIRQ_ST1633;
		/* pins used by touchscreen */
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_i2c4_st1633>;
		reg = <0x55>;
		reset-gpios = GP_ST1633_RESET;
		wakeup-gpios = GP_ST1633_IRQ;
	};

	wm8960: codec@1a {
		compatible = "wlf,wm8960";
		reg = <0x1a>;
		clocks = <&clk IMX8MQ_CLK_SAI1_ROOT>;
		clock-names = "mclk";
		wlf,shared-lrclk;
	};

	i2cmux@70 {
		compatible = "pca9546";
		reg = <0x70>;
		#address-cells = <1>;
		#size-cells = <0>;

		i2c4a: i2c4@0 {
			reg = <0>;
			#address-cells = <1>;
			#size-cells = <0>;
		};

		i2c4b: i2c4@1 {
			reg = <1>;
			#address-cells = <1>;
			#size-cells = <0>;
		};

		i2c4c: i2c4@2 {
			reg = <2>;
			#address-cells = <1>;
			#size-cells = <0>;
		};

		i2c4d: i2c4@3 {
			reg = <3>;
			#address-cells = <1>;
			#size-cells = <0>;
		};
	};
};

