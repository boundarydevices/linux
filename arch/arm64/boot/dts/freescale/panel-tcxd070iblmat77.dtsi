			DCS_S1P(0x23,0x00)
			DCS_S1P(0x24,0x00)
			DCS_S1P(0x25,0x00)
			DCS_S1P(0x26,0x00)
			DCS_S1P(0x27,0x00)
			DCS_S1P(0x28,0x33)	/* CLK Phase */
