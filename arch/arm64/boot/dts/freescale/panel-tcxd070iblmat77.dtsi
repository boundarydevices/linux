			DCS_S1P(0x2e,0x05)	/* GCH F */
			DCS_S1P(0x2f,0x05)	/* GCL F */
			DCS_S1P(0x30,0x00)
			DCS_S1P(0x31,0x00)
			DCS_S1P(0x32,0x31)	/* GCH/L ext2/1行 5E 01:31 5E 00:42 */
			DCS_S1P(0x33,0x00)
			DCS_S1P(0x34,0x00)
			DCS_S1P(0x35,0x0A)	/* GCH/L ^g 00:VS前 01:VS後 10:跨VS 11:frame中 */
			DCS_S1P(0x36,0x00)
			DCS_S1P(0x37,0x08)	/* GCH/L */
			DCS_S1P(0x38,0x00)
			DCS_S1P(0x39,0x00)
			DCS_S1P(0x3a,0x00)
