		mipi_cmds_tcxd070iblmat77 = &mipi_cmds_tcxd070iblmat77;
		pinctrl_tcxd070iblmat77 = &pinctrl_mipi_tcxd070iblmat77;
	};

	mipi_cmds_tcxd070iblmat77: mipi-cmds-tcxd070iblmat77 {
