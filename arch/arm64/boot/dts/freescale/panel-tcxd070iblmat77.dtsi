			DCS_S1P(0x76,0x08) 	/* BW_CGOUT_L[2] */
			DCS_S1P(0x77,0x0E) 	/* BW_CGOUT_L[3] */
			DCS_S1P(0x78,0x0F) 	/* BW_CGOUT_L[4] */
			DCS_S1P(0x79,0x0C) 	/* BW_CGOUT_L[5] */
			DCS_S1P(0x7a,0x0D) 	/* BW_CGOUT_L[6] */
			DCS_S1P(0x7b,0x17) 	/* BW_CGOUT_L[7] */
			DCS_S1P(0x7c,0x01) 	/* BW_CGOUT_L[8] */
			DCS_S1P(0x7d,0x01) 	/* BW_CGOUT_L[9] */
			DCS_S1P(0x7e,0x02) 	/* BW_CGOUT_L[10] */
			DCS_S1P(0x7f,0x02) 	/* BW_CGOUT_L[11] */
			DCS_S1P(0x80,0x00) 	/* BW_CGOUT_L[12] */
			DCS_S1P(0x81,0x00) 	/* BW_CGOUT_L[13] */
			DCS_S1P(0x82,0x02) 	/* BW_CGOUT_L[14] */
			DCS_S1P(0x83,0x02) 	/* BW_CGOUT_L[15] */
			DCS_S1P(0x84,0x16) 	/* BW_CGOUT_L[16] */
			DCS_S1P(0x85,0x16) 	/* BW_CGOUT_L[17] */
			DCS_S1P(0x86,0x06) 	/* BW_CGOUT_L[18] */
			DCS_S1P(0x87,0x06) 	/* BW_CGOUT_L[19] */
			DCS_S1P(0x88,0x07) 	/* BW_CGOUT_L[20] */
			DCS_S1P(0x89,0x07) 	/* BW_CGOUT_L[21] */
			DCS_S1P(0x8A,0x02) 	/* BW_CGOUT_L[22] */

			/* CMD_Page 4 */
			DCS_L3P(0xFF,0x98,0x81,0x04)

			DCS_S1P(0x6E,0x1A) 	/* VGH 12V */
			DCS_S1P(0x6F,0x37) 	/*  reg vcl + pumping ratio VGH=3x VGL=-3x */
			DCS_S1P(0x3A,0xA4) 	/* POWER SAVING */
			DCS_S1P(0x8D,0x1F) 	/* VGL -12V */
			DCS_S1P(0x87,0xBA) 	/* ESD */
			DCS_S1P(0xB2,0xD1)
			DCS_S1P(0x88,0x0B)
			DCS_S1P(0x38,0x01) 
			DCS_S1P(0x39,0x00)
			DCS_S1P(0xB5,0x02)	/* gamma bias */
			DCS_S1P(0x31,0x25) 	/* source bias */
			DCS_S1P(0x3B,0x98) 			
					
		 	/* CMD_Page 1 */
			DCS_L3P(0xFF,0x98,0x81,0x01)
			DCS_S1P(0x22,0x0A) 	/* BGR,0x SS */
			DCS_S1P(0x31,0x00) 	/* Column inversion */
			DCS_S1P(0x53,0x53) 	/* 3D VCOM1 20201129 */
			DCS_S1P(0x55,0x3D) 	/* VCOM2  */
			DCS_S1P(0x50,0x9E) 	/* A0 VREG1OUT 4.6V 20201203 */
			DCS_S1P(0x51,0x99) 	/* 9C VREG2OUT -4.6V 20201203 */
			DCS_S1P(0x60,0x06) 	/* SDT */
			DCS_S1P(0x62,0x20)

		 	/* ============Gamma START============= */
		 	/* Pos Register 20201203 */
			DCS_S1P(0xA0,0x00)
			DCS_S1P(0xA1,0x17)
			DCS_S1P(0xA2,0x26)
			DCS_S1P(0xA3,0x13)
			DCS_S1P(0xA4,0x16)
			DCS_S1P(0xA5,0x29)
			DCS_S1P(0xA6,0x1E)
			DCS_S1P(0xA7,0x1F)
			DCS_S1P(0xA8,0x8B)
			DCS_S1P(0xA9,0x1D)
			DCS_S1P(0xAA,0x2A)
			DCS_S1P(0xAB,0x7B)
			DCS_S1P(0xAC,0x1A)
			DCS_S1P(0xAD,0x19)
			DCS_S1P(0xAE,0x4E)
			DCS_S1P(0xAF,0x24)
			DCS_S1P(0xB0,0x29)
			DCS_S1P(0xB1,0x4F)
			DCS_S1P(0xB2,0x5C)
			DCS_S1P(0xB3,0x23)
										 
		 	/* Neg Register */
			DCS_S1P(0xC0,0x00)
			DCS_S1P(0xC1,0x17)
			DCS_S1P(0xC2,0x26)
			DCS_S1P(0xC3,0x13)
			DCS_S1P(0xC4,0x16)
			DCS_S1P(0xC5,0x29)
			DCS_S1P(0xC6,0x1E)
			DCS_S1P(0xC7,0x1F)
			DCS_S1P(0xC8,0x8B)
			DCS_S1P(0xC9,0x1D)
			DCS_S1P(0xCA,0x2A)
			DCS_S1P(0xCB,0x7B)
			DCS_S1P(0xCC,0x1A)
			DCS_S1P(0xCD,0x19)
			DCS_S1P(0xCE,0x4E)
			DCS_S1P(0xCF,0x24)
			DCS_S1P(0xD0,0x29)
			DCS_S1P(0xD1,0x4F)
			DCS_S1P(0xD2,0x5C)
			DCS_S1P(0xD3,0x23)
		 	/* ============ Gamma END=========== */

			/* CMD_Page 0 */
			DCS_L3P(0xFF,0x98,0x81,0x00)
		 	/* --- TE---- */
			DCS_S1P(0x35,0x00)
			DCS_S0P(0x11)		/* Exit sleep */
		>;
		mipi-cmds-enable2 = /bits/ 8 <
			DELAY(120)
			DCS_S0P(0x29)		/* MIPI_DCS_SET_DISPLAY_ON */
			DELAY(120)
		>;
		mipi-cmds-disable = /bits/ 8 <
			DELAY(50)
			DCS_S0P(0x28)		/* MIPI_DCS_SET_DISPLAY_OFF */
			DELAY(20)
			DCS_S0P(0x10)		/* Enter sleep */
			DELAY(1)
		>;
		mode-video-hfp-disable;
		mode-video-hsa-disable;
	};
};
