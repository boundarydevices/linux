			DCS_S1P(0x44,0x00)
										 
		 	/* GIP_2 */
			DCS_S1P(0x50,0x01)
			DCS_S1P(0x51,0x23)
			DCS_S1P(0x52,0x44)
			DCS_S1P(0x53,0x67)
			DCS_S1P(0x54,0x89)
			DCS_S1P(0x55,0xab)
			DCS_S1P(0x56,0x01)
			DCS_S1P(0x57,0x23)
			DCS_S1P(0x58,0x45)
			DCS_S1P(0x59,0x67)
			DCS_S1P(0x5a,0x89)
			DCS_S1P(0x5b,0xab)
			DCS_S1P(0x5c,0xcd)
			DCS_S1P(0x5d,0xef)
				 
		 	/* GIP_3 */
			DCS_S1P(0x5e,0x11)
			DCS_S1P(0x5f,0x02) 	/* FW_CGOUT_L[1] */
			DCS_S1P(0x60,0x08) 	/* FW_CGOUT_L[2] STV0 */
			DCS_S1P(0x61,0x0E) 	/* FW_CGOUT_L[3] CLK1 */
			DCS_S1P(0x62,0x0F) 	/* FW_CGOUT_L[4] CLK3 */
			DCS_S1P(0x63,0x0C) 	/* FW_CGOUT_L[5] CLK5 */
			DCS_S1P(0x64,0x0D) 	/* FW_CGOUT_L[6] CLK7 */
