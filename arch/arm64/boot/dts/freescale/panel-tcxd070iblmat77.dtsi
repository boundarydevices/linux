			/* LCD driver initialization */
			/* 10=TX_CLK 30=PCLK */
			DCS_L2P(0xB7,0x50,0x02)
			DCS_L2P(0xBD,0x00,0x00)
			/* Page 3*/
			DCS_L3P(0xFF,0x98,0x81,0x03)
			/* GIP_1 */
			DCS_S1P(0x01,0x00)
			DCS_S1P(0x02,0x00)
			DCS_S1P(0x03,0x73)	/* STVA=STV1~4 */
			DCS_S1P(0x04,0x13)	/* STVB=STV0 */
			DCS_S1P(0x05,0x00)
			DCS_S1P(0x06,0x0A)	/* STVA_Rise */
			DCS_S1P(0x07,0x05)	/* STVB_Rise */
