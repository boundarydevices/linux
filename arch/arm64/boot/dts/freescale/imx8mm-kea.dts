&gpu {
	status = "okay";
};

&i2c1 {
	clock-frequency = <400000>;
	pinctrl-names = "default", "gpio";
	pinctrl-0 = <&pinctrl_i2c1>;
	pinctrl-1 = <&pinctrl_i2c1_1>;
	scl-gpios = GP_I2C1_SCL;
	sda-gpios = GP_I2C1_SDA;
	status = "okay";

	pf8100@8 {
		compatible = "nxp,pf8100";
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_i2c1_pf8100>;
		reg = <0x08>;

		regulators {
			reg_ldo1: ldo1 {
				/* LDO1_3P3V, unused */
				regulator-max-microvolt = <5000000>;
				regulator-min-microvolt = <1500000>;

				regulator-state-standby {
					regulator-off-in-suspend;
				};

				regulator-state-mem {
					regulator-off-in-suspend;
				};

				regulator-state-disk {
					regulator-off-in-suspend;
				};
			};

			reg_ldo2: ldo2 {
				/* NVCC_SD2, SD Card */
				regulator-max-microvolt = <5000000>;
				regulator-min-microvolt = <1500000>;
#if 0
				/* vselect low is 3.3V, high is 1.8V */
				vselect-en;
#endif

				regulator-state-standby {
					regulator-off-in-suspend;
				};

				regulator-state-mem {
					regulator-off-in-suspend;
				};

				regulator-state-disk {
					regulator-off-in-suspend;
				};
			};

			reg_ldo3: ldo3 {
				/* LDO3_1P8V_TO_3P3V, unused */
				regulator-max-microvolt = <5000000>;
				regulator-min-microvolt = <1500000>;

				regulator-state-standby {
					regulator-off-in-suspend;
				};

				regulator-state-mem {
					regulator-off-in-suspend;
				};

				regulator-state-disk {
					regulator-off-in-suspend;
				};
			};

			reg_ldo4: ldo4 {
				/* VDDA_1V8 */
				regulator-always-on;
				regulator-boot-on;
				regulator-max-microvolt = <5000000>;
				regulator-min-microvolt = <1500000>;

				regulator-state-standby {
					regulator-off-in-suspend;
				};

				regulator-state-mem {
					regulator-off-in-suspend;
				};

				regulator-state-disk {
					regulator-off-in-suspend;
				};
			};

			reg_buck1: buck1 {
				/* VDD_SOC */
				nxp,phase-shift = <0>;
				regulator-max-microamp = <4500000>;
				regulator-always-on;
				regulator-boot-on;
				regulator-max-microvolt = <1800000>;
				regulator-min-microvolt =  <400000>;

				regulator-state-standby {
					regulator-on-in-suspend;
					regulator-suspend-max-microvolt = <1800000>;
					regulator-suspend-microvolt = <781250>;
					regulator-suspend-min-microvolt =  <400000>;
				};

				regulator-state-mem {
					regulator-on-in-suspend;
					regulator-suspend-max-microvolt = <1800000>;
					regulator-suspend-microvolt = <781250>;
					regulator-suspend-min-microvolt =  <400000>;
				};

				regulator-state-disk {
					regulator-on-in-suspend;
					regulator-suspend-max-microvolt = <1800000>;
					regulator-suspend-microvolt = <781250>;
					regulator-suspend-min-microvolt =  <400000>;
				};
			};

			reg_buck2: buck2 {
				/* VDD_GPU_DRAM, processor only */
				regulator-always-on;
				regulator-boot-on;
				regulator-max-microvolt = <1800000>;
				regulator-min-microvolt =  <400000>;

				regulator-state-standby {
					regulator-off-in-suspend;
				};

				regulator-state-mem {
					regulator-off-in-suspend;
				};

				regulator-state-disk {
					regulator-off-in-suspend;
				};
			};

			reg_buck3: buck3 {
				/* VDD_ARM */
				regulator-always-on;
				regulator-boot-on;
				regulator-max-microvolt = <1800000>;
				regulator-min-microvolt =  <400000>;
				fast-slew = <1>;

				regulator-state-standby {
					regulator-off-in-suspend;
				};

				regulator-state-mem {
					regulator-off-in-suspend;
				};

				regulator-state-disk {
					regulator-off-in-suspend;
				};
			};

			reg_buck4: buck4 {
				/* AUX_0P8V, unused */
				regulator-max-microvolt = <1800000>;
				regulator-min-microvolt =  <400000>;
				fast-slew = <1>;

				regulator-state-standby {
					regulator-off-in-suspend;
				};

				regulator-state-mem {
					regulator-off-in-suspend;
				};

				regulator-state-disk {
					regulator-off-in-suspend;
				};
			};

			reg_buck5: buck5 {
				/* VDD_VPU */
				regulator-always-on;
				regulator-boot-on;
				regulator-max-microvolt = <1800000>;
				regulator-min-microvolt =  <400000>;

				regulator-state-standby {
					regulator-off-in-suspend;
				};

				regulator-state-mem {
					regulator-off-in-suspend;
				};

				regulator-state-disk {
					regulator-off-in-suspend;
				};
			};

			reg_buck6: buck6 {
				/* NVCC_DRAM_1P1V */
				regulator-always-on;
				regulator-boot-on;
				regulator-max-microvolt = <1800000>;
				regulator-min-microvolt =  <400000>;

				regulator-state-standby {
					regulator-on-in-suspend;
					regulator-suspend-max-microvolt = <1800000>;
					regulator-suspend-microvolt = <1062500>;
					regulator-suspend-min-microvolt =  <400000>;
				};

				regulator-state-mem {
					regulator-on-in-suspend;
					regulator-suspend-max-microvolt = <1800000>;
					regulator-suspend-microvolt = <1062500>;
					regulator-suspend-min-microvolt =  <400000>;
				};

				regulator-state-disk {
					regulator-on-in-suspend;
					regulator-suspend-max-microvolt = <1800000>;
					regulator-suspend-microvolt = <1062500>;
					regulator-suspend-min-microvolt =  <400000>;
				};
			};

			reg_buck7: buck7 {
				/* 3P3V */
				regulator-always-on;
				regulator-boot-on;
				regulator-max-microvolt = <4100000>;
				regulator-min-microvolt = <1000000>;

				regulator-state-standby {
					regulator-on-in-suspend;
				};

				regulator-state-mem {
					regulator-on-in-suspend;
				};

				regulator-state-disk {
					regulator-on-in-suspend;
				};
			};

			reg_vsnvs: vsnvs {
				/* NVCC_SNVS_1P8V */
				regulator-always-on;
				regulator-boot-on;
				regulator-max-microvolt = <3300000>;
				regulator-min-microvolt = <1800000>;
			};
		};
	};
};

