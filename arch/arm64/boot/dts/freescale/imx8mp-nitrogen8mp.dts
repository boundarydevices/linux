					remote-endpoint = <&hd3ss3220_in_ep>;
				};
			};
		};
	};

	gpio_keys: gpio-keys {
		compatible = "gpio-keys";
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_key_can1>, <&pinctrl_key_can2>;

		can1 {
			label = "can1 detect";
			gpios = GP_CAN1_DETECT;
			linux,code = <KEY_1>;
		};

		can2 {
			label = "can2 detect";
			gpios = GP_CAN2_DETECT;
			linux,code = <KEY_2>;
		};
	};

