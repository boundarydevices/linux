	sda-gpios = GP_I2C4_SDA;
	status = "okay";

	i2cmux@70 {
		compatible = "nxp,pca9546";
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_i2c4_tca9546>;
		reg = <0x70>;
		reset-gpios = GP_I2C4_MUX_RESET;
		#address-cells = <1>;
		#size-cells = <0>;

		i2c4a: i2c4@0 {
			/* usb type c chip */
			reg = <0>;
			#address-cells = <1>;
			#size-cells = <0>;
		};

		i2c4b: i2c4@1 {
			/* spare */
			reg = <1>;
			#address-cells = <1>;
			#size-cells = <0>;
		};

		i2c4c: i2c4@2 {
			/* J6 connector */
			reg = <2>;
			#address-cells = <1>;
			#size-cells = <0>;
		};

		i2c4d: i2c4@3 {
			/* rtc */
			reg = <3>;
			#address-cells = <1>;
			#size-cells = <0>;
		};
	};
};

