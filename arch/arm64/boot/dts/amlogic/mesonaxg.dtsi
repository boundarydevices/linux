		};
	};

	b_uart_pins:b_uart {
		mux {
			pins = "GPIOZ_2",
				"GPIOZ_3";
			function = "uart_b";
		};
	};

	a_i2c_master:a_i2c {
		mux {
			pins = "GPIOZ_6","GPIOZ_7";
			function = "i2c_a";
		};
	};

	b_i2c_master:b_i2c {
		mux {
			pins = "GPIOZ_8","GPIOZ_9";
			function = "i2c_b";
		};
	};

	b_i2c_master_pin1:b_i2c_pin1 {
		mux {
			pins = "GPIOX_16","GPIOX_17";
			function = "i2c_b";
		};
	};

	c_i2c_master:c_i2c {
		mux {
			pins = "GPIOX_18","GPIOX_19";
			function = "i2c_c";
		};
	};

	c_i2c_master_pin1:c_i2c_pin1 {
		mux {
			pins = "GPIOA_17","GPIOA_18";
			function = "i2c_c";
		};
	};

	d_i2c_master:d_i2c {
		mux {
			pins = "GPIOA_6","GPIOA_7";
