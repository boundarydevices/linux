				"GPIOY_2",
				"GPIOY_3",
				"GPIOY_4",
				"GPIOY_5",
				"GPIOY_6",
				"GPIOY_7",
				"GPIOY_8",
				"GPIOY_9",
				"GPIOY_10",
				"GPIOY_11",
				"GPIOY_12",
				"GPIOY_13";
			function = "eth";
		};
	};
	ao_b_uart_pins:ao_b_uart {
		mux {
				pins = "GPIOZ_8",
					"GPIOZ_9";
				function = "uart_ao_b";
