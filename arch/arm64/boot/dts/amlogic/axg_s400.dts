/* Audio Related start */
/* for spk board */
&i2c_b {
	status = "okay";
	//pinctrl-names="default";
	//pinctrl-0=<&b_i2c_master>;
	tlv320adc3101_32: tlv320adc3101_32@32 {
		compatible = "ti,tlv320adc3101";
