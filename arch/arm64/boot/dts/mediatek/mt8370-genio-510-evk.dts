	vusb33-supply = <&mt6359_vusb_ldo_reg>;
	status = "okay";
	wakeup-source;
};

&u2port2 {
	status = "okay";
};

&xhci2 {
	status = "okay";
	vusb33-supply = <&mt6359_vusb_ldo_reg>;
};

&cam_vcore {
	domain-supply = <&mt6359_vproc1_buck_reg>;
};

&img_vcore {
	domain-supply = <&mt6359_vproc1_buck_reg>;
};

&accdet {
	mediatek,eint-level-pol = <IRQ_TYPE_LEVEL_LOW>;
};

&afe {
	#sound-dai-cells = <0>;
	memory-region = <&afe_dma_mem_reserved>;
	mediatek,dmic-iir-on;
	mediatek,dmic-clk-mono;
	status = "okay";
};

&adsp {
	memory-region = <&adsp_dma_mem_reserved>,
		<&adsp_mem_reserved>;
};

&sound {
	compatible = "mediatek,mt8188-mt6359";
	model = "mt8370-evk";
	pinctrl-names = "default";
	pinctrl-0 = <&audio_default_pins>;
	audio-routing =
		"Headphone", "Headphone L",
		"Headphone", "Headphone R",
		"AIN1", "Headset Mic",
		"DMIC_INPUT", "AP DMIC",
		"AP DMIC", "AUDGLB",
		"AP DMIC", "MIC_BIAS_0",
		"AP DMIC", "MIC_BIAS_2";
	status = "okay";

	dai-link-0 {
		sound-dai = <&afe>;
		link-name = "DL_SRC_BE";

		codec {
			sound-dai = <&pmic 0>, <&accdet>;
		};
	};

	dai-link-1 {
		sound-dai = <&afe>;
		link-name = "UL_SRC_BE";

		codec {
			sound-dai = <&pmic 0>, <&amic_codec>;
		};
	};

	dai-link-2 {
		sound-dai = <&afe>;
		link-name = "DMIC_BE";

		codec {
			sound-dai = <&dmic_codec>;
		};
	};
};
