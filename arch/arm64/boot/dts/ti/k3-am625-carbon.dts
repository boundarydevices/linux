/dts-v1/;

#include <dt-bindings/leds/common.h>
#include "k3-am625-carbon-som.dtsi"

/ {
	model = "Ezurio Carbon AM625 OSM Carrier Board";
	compatible = "ezurio,am62-dev",
		     "ti,am625";

	vdd_mmc1: regulator-sdio-a {
		bootph-all;
		compatible = "regulator-fixed";
		pinctrl-names = "default";
		pinctrl-0 = <&main_vdd_sdio_a_gpio>;
		regulator-name = "vdd_mmc1";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		startup-delay-us = <100000>;
		regulator-boot-on;
		enable-active-high;
		gpio = <&main_gpio0 18 GPIO_ACTIVE_HIGH>;
	};

	vdd_3v3: regulator-u4-sw2 {
		/* output of U4 */
		bootph-all;
		compatible = "regulator-fixed";
		regulator-name = "vdd_3v3";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		regulator-always-on;
		regulator-boot-on;
	};

	vdd_1v8: regulator-u4-sw3 {
		/* output of U4 */
		bootph-all;
		compatible = "regulator-fixed";
		regulator-name = "vdd_1v8";
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		regulator-always-on;
		regulator-boot-on;
	};

