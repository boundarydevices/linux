			MX6QDL_PAD_KEY_COL0__GPIO4_IO06		0x1b0b0
		>;
	};

	pinctrl_sata: satagrp {
		fsl,pins = <
#define GP_HD_DETECT			<&gpio2 30 GPIO_ACTIVE_HIGH>
