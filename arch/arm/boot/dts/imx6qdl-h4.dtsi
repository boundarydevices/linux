#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

&iomuxc {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_hog>;

	iomuxc_imx6q_h4: iomuxc-imx6q-h4grp {
		status = "okay";
	};
};

&iomuxc_imx6q_h4 {
