/dts-v1/;
/include/ "imx6q.dtsi"

/ {
	model = "Freescale i.MX6Q SABRE Smart Device Board";
	compatible = "fsl,imx6q-sabresd", "fsl,imx6q";

	memory {
		reg = <0x10000000 0x40000000>;
	};

	soc {
		pcie@01000000 {
			pwr-gpios = <&gpio3 19 0>;
			rst-gpios = <&gpio7 12 0>;
			status = "okay";
		};

		aips-bus@02000000 { /* AIPS1 */
			spba-bus@02000000 {
				uart1: serial@02020000 {
					pinctrl-names = "default";
					pinctrl-0 = <&pinctrl_uart1_1>;
					status = "okay";
				};

				ssi2: ssi@0202c000 {
					fsl,mode = "i2s-slave";
					status = "okay";
				};

				ecspi@02008000 { /* eCSPI1 */
					fsl,spi-num-chipselects = <1>;
					cs-gpios = <&gpio4 9 0>;
					pinctrl-names = "default";
					pinctrl-0 = <&pinctrl_ecspi1_2>;
					status = "okay";

					flash: m25p32@0 {
						#address-cells = <1>;
						#size-cells = <1>;
						compatible = "st,m25p32", "st,m25p";
						spi-max-frequency = <20000000>;
						reg = <0>;

						partition@0 {
							label = "U-Boot";
							reg = <0x0 0x100000>;
							read-only;
						};

						partition@40000 {
							label = "Kernel";
							reg = <0x40000 0x300000>;
						};
					};
				};
			};

			pwm: pwm@02080000 {
				pinctrl-names = "default";
				pinctrl-0 = <&pinctrl_pwm0_0>;
				status = "okay";
			};

			iomuxc@020e0000 {
				pinctrl-names = "default";
				pinctrl-0 = <&pinctrl_hog>;

				hog {
					pinctrl_hog: hoggrp {
						fsl,pins = <
							1402 0x80000000	/* MX6Q_PAD_NANDF_D0__GPIO_2_0 */
							1410 0x80000000	/* MX6Q_PAD_NANDF_D1__GPIO_2_1 */
							1418 0x80000000	/* MX6Q_PAD_NANDF_D2__GPIO_2_2 */
							1426 0x80000000	/* MX6Q_PAD_NANDF_D3__GPIO_2_3 */
							1341 0x80000000 /* MX6Q_PAD_NANDF_ALE__GPIO_6_8 */
							1333 0x80000000 /* MX6Q_PAD_NANDF_CLE__GPIO_6_7 */
							184 0x80000000 /* MX6Q_PAD_EIM_D26__GPIO_3_26 */
							305 0x80000000 /* MX6Q_PAD_EIM_CS1__GPIO_2_24 */
							733 0x80000000 /* MX6Q_PAD_ENET_RXD0__GPIO_1_27 */
							82  0x80000000 /* MX6Q_PAD_EIM_A25__GPIO_5_2 */
							152 0x80000000 /* MX6Q_PAD_EIM_D23__GPIO_3_23 */
							961 0x80000000	/* MX6Q_PAD_GPIO_1__WDOG2_WDOG_B */
							1357 0x80000000 /* MX6Q_PAD_NANDF_RB0__GPIO_6_10 */
							902 0x80000000 /* MX6Q_PAD_KEY_ROW1__GPIO_4_9 */
							910  0x80000000	/* MX6Q_PAD_KEY_COL2__GPIO_4_10 */
							1325 0x80000000	/* MX6Q_PAD_SD3_RST__GPIO_7_8 */
							972  0x80000000	/* MX6Q_PAD_GPIO_9__GPIO_1_9 */
							953 0x130b0  /* MX6Q_PAD_GPIO_0__CCM_CLKO */
						>;
					};
				};
			};
		};

		aips-bus@02100000 { /* AIPS2 */
			usb@02184000 { /* USB OTG */
				vbus-supply = <&reg_usb_otg_vbus>;
				pinctrl-names = "default";
				pinctrl-0 = <&pinctrl_usbotg_2>;
				disable-over-current;
				status = "okay";
			};

			usb@02184200 { /* USB1 */
				status = "okay";
			};

			ethernet@02188000 {
				pinctrl-names = "default";
				pinctrl-0 = <&pinctrl_enet_1>;
				phy-mode = "rgmii";
				status = "okay";
			};

			usdhc@02194000 { /* uSDHC2 */
				pinctrl-names = "default";
				pinctrl-0 = <&pinctrl_usdhc2_1>;
				cd-gpios = <&gpio2 2 0>;
				wp-gpios = <&gpio2 3 0>;
				bus-width = <8>;
				no-1-8-v;
				keep-power-in-suspend;
				enable-sdio-wakeup;
				status = "okay";
			};

			usdhc@02198000 { /* uSDHC3 */
				pinctrl-names = "default";
				pinctrl-0 = <&pinctrl_usdhc3_1>;
				cd-gpios = <&gpio2 0 0>;
				wp-gpios = <&gpio2 1 0>;
				bus-width = <8>;
				no-1-8-v;
				keep-power-in-suspend;
				enable-sdio-wakeup;
				status = "okay";
			};

			usdhc@0219c000 { /* uSDHC4 */
				pinctrl-names = "default";
				pinctrl-0 = <&pinctrl_usdhc4_1>;
				non-removable;
				bus-width = <8>;
				no-1-8-v;
				keep-power-in-suspend;
				enable-sdio-wakeup;
				status = "okay";
			};

			audmux@021d8000 {
				status = "okay";
				pinctrl-names = "default";
				pinctrl-0 = <&pinctrl_audmux_2>;
			};

			i2c@021a0000 { /* I2C1 */
				status = "okay";
				clock-frequency = <100000>;
				pinctrl-names = "default";
				pinctrl-0 = <&pinctrl_i2c1_2>;

				codec: wm8962@1a {
					compatible = "fsl,wm8962";
					reg = <0x1a>;
					clocks = <&clks 169>;
					DCVDD-supply = <&vgen4_reg>;	/* 1.8v */
					DBVDD-supply = <&vgen4_reg>;	/* 1.8v */
					AVDD-supply = <&vgen4_reg>;		/* 1.8v */
					CPVDD-supply = <&vgen4_reg>;	/* 1.8v */
					MICVDD-supply = <&vgen6_reg>;	/* 3.3v */
					PLLVDD-supply = <&vgen4_reg>;	/* 1.8v */
					SPKVDD1-supply = <&reg_4p2v>;
					SPKVDD2-supply = <&reg_4p2v>;
					gpio-cfg = <
						0x0000 /* 0:Default */
						0x0000 /* 1:Default */
						0x0013 /* 2:FN_DMICCLK */
						0x0000 /* 3:Default */
						0x8014 /* 4:FN_DMICCDAT */
						0x0000 /* 5:Default */
					>;
				};
			};

			i2c@021a4000 { /* I2C2 */
                                status = "okay";
                                clock-frequency = <100000>;
                                pinctrl-names = "default";
                                pinctrl-0 = <&pinctrl_i2c2_2>;

				max11801@48 {
					compatible = "fsl,max11801";
					reg = <0x48>;
					int_input = <&gpio3 26 1>;
                                };

				egalax_ts@04 {
					compatible = "eeti,egalax_ts";
					reg = <0x04>;
					interrupt-parent = <&gpio6>;
					interrupts = <8 2>;
					wakeup-gpios = <&gpio6 8 0>;
				};

				pmic: pfuze100@08 {
					compatible = "fsl,pfuze100";
					reg = <0x08>;

					regulators {
						sw1a_reg: pfuze100__sw1a {
							regulator-min-microvolt = <300000>;
							regulator-max-microvolt = <1875000>;
							regulator-standby-microvolt = <18000>;
							regulator-boot-on;
							regulator-always-on;
						};

						sw1c_reg: pfuze100__sw1c {
							regulator-min-microvolt = <300000>;
							regulator-max-microvolt = <1875000>;
							regulator-boot-on;
							regulator-always-on;
						};

						sw2_reg: pfuze100__sw2 {
							regulator-min-microvolt = <800000>;
							regulator-max-microvolt = <3950000>;
							regulator-boot-on;
							regulator-always-on;
						};

						sw3a_reg: pfuze100__sw3a {
							regulator-min-microvolt = <400000>;
							regulator-max-microvolt = <1975000>;
							regulator-boot-on;
							regulator-always-on;
						};

						sw3b_reg: pfuze100__sw3b {
							regulator-min-microvolt = <400000>;
							regulator-max-microvolt = <1975000>;
							regulator-boot-on;
							regulator-always-on;
						};

						sw4_reg: pfuze100__sw4 {
							regulator-min-microvolt = <800000>;
							regulator-max-microvolt = <3950000>;
						};

						swbst_reg: pfuze100__swbst {
							regulator-min-microvolt = <5000000>;
							regulator-max-microvolt = <5150000>;
						};

						snvs_reg: pfuze100__vsnvs {
							regulator-min-microvolt = <1200000>;
							regulator-max-microvolt = <3000000>;
							regulator-boot-on;
							regulator-always-on;
						};

						vref_reg: pfuze100__vrefddr {
							regulator-boot-on;
							regulator-always-on;
						};

						vgen1_reg: pfuze100__vgen1 {
							regulator-min-microvolt = <800000>;
							regulator-max-microvolt = <1550000>;
						};

						vgen2_reg: pfuze100__vgen2 {
							regulator-min-microvolt = <800000>;
							regulator-max-microvolt = <1550000>;
						};

						vgen3_reg: pfuze100__vgen3 {
							regulator-min-microvolt = <1800000>;
							regulator-max-microvolt = <3300000>;
						};

						vgen4_reg: pfuze100__vgen4 {
							regulator-min-microvolt = <1800000>;
							regulator-max-microvolt = <3300000>;
						};

						vgen5_reg: pfuze100__vgen5 {
							regulator-min-microvolt = <1800000>;
							regulator-max-microvolt = <3300000>;
						};

						vgen6_reg: pfuze100__vgen6 {
							regulator-min-microvolt = <1800000>;
							regulator-max-microvolt = <3300000>;
						};
					};
				};
			};

			i2c@021a8000 { /* I2C3 */
				status = "okay";
				clock-frequency = <100000>;
				pinctrl-names = "default";
				pinctrl-0 = <&pinctrl_i2c3_2>;

				egalax_ts@04 {
					compatible = "eeti,egalax_ts";
					reg = <0x04>;
					interrupt-parent = <&gpio6>;
					interrupts = <7 2>;
					wakeup-gpios = <&gpio6 7 0>;
				};
			};
		};

		ahci@02200000 { /* AHCI SATA */
			status = "okay";
		};

	regulator-bypass {
			compatible = "fsl,imx_cpufreq";
			/*
			 * 1:bypass internal regulator,supplied by external reg
			 * 0:enable internal regulator
			 */
			bypass-mode = <1>;
			vddarm-supply = <&sw1a_reg>;
			vddsoc-supply = <&sw1c_reg>;
		};
	};

	regulators {
		compatible = "simple-bus";

		reg_4p2v: 4p2v {
			compatible = "regulator-fixed";
			regulator-name = "4P2V";
			regulator-min-microvolt = <4200000>;
			regulator-max-microvolt = <4200000>;
			gpio = <&gpio4 10 0>;
			regulator-always-on;
			regulator-boot-on;
			enable-active-high;
		};

		aux_5v: aux5v {
			compatible = "regulator-fixed";
			regulator-name = "AUX_5V";
			regulator-min-microvolt = <5000000>;
			regulator-max-microvolt = <5000000>;
			gpio = <&gpio6 10 1>;
			regulator-boot-on;
			enable-active-high;
		};

		reg_usb_otg_vbus: usb_otg_vbus {
			compatible = "regulator-fixed";
			regulator-name = "usb_otg_vbus";
			regulator-min-microvolt = <5000000>;
			regulator-max-microvolt = <5000000>;
			gpio = <&gpio3 22 0>;
			enable-active-high;
		};
	};

	sound {
		compatible = "fsl,imx6q-sabresd-wm8962",
			     "fsl,imx-audio-wm8962";
		model = "imx6q-sabresd-wm8962";
		ssi-controller = <&ssi2>;
		audio-codec = <&codec>;

		audio-routing =
			"Headphone Jack", "HPOUTL",
			"Headphone Jack", "HPOUTR",
			"Ext Spk", "SPKOUTL",
			"Ext Spk", "SPKOUTR",
			"MICBIAS", "AMIC",
			"IN3R", "MICBIAS",
			"DMIC", "MICBIAS",
			"DMICDAT", "DMIC";
		mux-int-port = <2>;
		mux-ext-port = <3>;
		hp-det-gpios = <&gpio7 8 1>;	/*active low*/
		mic-det-gpios = <&gpio1 9 1>;	/*active low*/
	};

	battery {
		compatible = "fsl,max8903-charger";
		pinctrl-names = "default";
		dok_input = <&gpio2 24 1>;
		uok_input = <&gpio1 27 1>;
		chg_input = <&gpio3 23 1>;
		flt_input = <&gpio5 2 1>;
		fsl,dcm_always_high;
		fsl,dc_valid;
		fsl,usb_valid;
		fsl,imx6q;
		status = "okay";
	};

	backlight {
		compatible = "pwm-backlight";
		pwms = <&pwm 0 5000000>;
		brightness-levels = <0 4 8 16 32 64 128 255>;
		default-brightness-level = <7>;
	};
};
