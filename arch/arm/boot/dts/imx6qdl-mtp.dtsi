		fsl,pins = <
#define GP_WL1271_BT_EN		<&gpio6 16 GPIO_ACTIVE_HIGH>
			MX6QDL_PAD_NANDF_CS3__GPIO6_IO16	0x000b0
			MX6QDL_PAD_KEY_ROW4__GPIO4_IO15		0x1b0b0		/* wl1271 btfunc5 */
