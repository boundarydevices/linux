&esai {
	/* pin conflict with sai */
	status = "disabled";
};

&sai1 {
	status = "okay";
};

&sdma {
	/* SDMA event remap for SAI1 */
	fsl,sdma-event-remap = <0 15 1>, <0 16 1>;
};
