	hdmi: edid@50 {
		compatible = "fsl,imx6-hdmi-i2c";
		reg = <0x50>;
	};
};

&i2c3 {
        clock-frequency = <100000>;
        pinctrl-names = "default";
        pinctrl-0 = <&pinctrl_i2c3>;
        status = "okay";

        egalax_ts@04 {
                compatible = "eeti,egalax_ts";
                reg = <0x04>;
		interrupts-extended = GP_TOUCH_IRQ; 
                wakeup-gpios = GP_TOUCH_IRQ;
        };

        ft5x06_ts@38 {
		compatible = "ft5x06-ts,ft5x06-ts";
		reg = <0x38>;
		interrupts-extended = GP_TOUCH_IRQ; 
                wakeup-gpios = GP_TOUCH_IRQ;
	};

        ili210x@41 {
		compatible = "ili210x";
		reg = <0x41>;
		interrupts-extended = GP_TOUCH_IRQ; 
                wakeup-gpios = GP_TOUCH_IRQ;
	};
};

&i2c3mux {
	mux-gpios = GP_I2C3_PCIE_EN, GP_I2C3_MAX77818_EN;
	i2c-parent = <&i2c3>;
	idle-state = <0>;

	i2c3@0 {
		reg = <0>;
		#address-cells = <1>;
		#size-cells = <0>;
	};

	i2c3@1 {
		/* PCIe */
		reg = <1>;
		#address-cells = <1>;
		#size-cells = <0>;
	};

	i2c3@2 {
		/* MAX77818 */		
		reg = <2>;
		#address-cells = <1>;
		#size-cells = <0>;
		max77818@69 {
			compatible = "max77818";
			reg = <0x69>;
			interrupts-extended = GP_MAX77818_INTB;
		};
	};
};

