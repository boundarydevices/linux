			MX6QDL_PAD_DI0_PIN4__GPIO4_IO20		0x030b0
#define GP_I2C1_TCA9546_RESET	<&gpio1 8 GPIO_ACTIVE_LOW>
			MX6QDL_PAD_GPIO_8__GPIO1_IO08		0x030b0
		>;
	};

