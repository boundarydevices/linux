			MX6QDL_PAD_DISP0_DAT3__GPIO4_IO24	0x030b0
#define GP_WM8960_MIC_DET	<&gpio7 8 GPIO_ACTIVE_LOW>
			MX6QDL_PAD_SD3_RST__GPIO7_IO08		0x1b0b0
			MX6QDL_PAD_GPIO_0__CCM_CLKO1		0x000b0		/* mclk */
		>;
	};

