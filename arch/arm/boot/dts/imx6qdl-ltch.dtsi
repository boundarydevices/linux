		pinctrl-0 = <&pinctrl_i2c1b_ft5x06>;
		reg = <0x38>;
		interrupts-extended = GPIRQ_I2C1B_J7;
		wakeup-gpios = GP_I2C1B_J7;
	};
};

&i2c1c {
	clock-frequency = <100000>;
	/* to pcie connector */
};

&i2c1d {
	clock-frequency = <100000>;

	ft5x06-ts@38 {
		compatible = "ft5x06-ts";
